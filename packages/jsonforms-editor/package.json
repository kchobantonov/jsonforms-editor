{
  "name": "@jsonforms/editor",
  "version": "0.1.0",
  "description": "JSON Forms Editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eclipsesource/jsonforms-editor.git",
    "directory": "packages/jsonforms-editor"
  },
  "bugs": "https://github.com/eclipsesource/jsonforms-editor/issues",
  "homepage": "http://jsonforms.io/",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "src": "src",
    "test": "test"
  },
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/jsonforms-editor.cjs.js",
  "module": "lib/jsonforms-editor.esm.js",
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@react-spring/web": "^9.7.5",
    "ajv": "^8.6.1",
    "json-schema-traverse": "^1.0.0",
    "lodash": "^4.17.21",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-redux": "^7.1.3",
    "react-reflex": "^4.2.7",
    "redux": "^4.0.5",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@jsonforms/core": "3.7.0-alpha.3",
    "@jsonforms/material-renderers": "3.7.0-alpha.3",
    "@jsonforms/react": "3.7.0-alpha.3",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@mui/x-date-pickers": "^8.0.0",
    "@mui/x-tree-view": "^7.29.10",
    "monaco-editor": "0.52.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-monaco-editor": "^0.59.0",
    "tslib": "^2.7.0"
  },
  "scripts": {
    "build": "rimraf lib && rollup --config rollup.config.js",
    "watch": "rimraf lib && rollup -cw",
    "test": "jest --no-cache",
    "test-cov": "jest --no-cache --coverage",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@jsonforms/core": "3.7.0-alpha.3",
    "@jsonforms/react": "3.7.0-alpha.3",
    "@mui/icons-material": "^7.3.0",
    "@mui/material": "^7.3.0",
    "@mui/x-date-pickers": "^8.11.3",
    "@mui/x-tree-view": "^7.29.10",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/json-schema-traverse": "^1.0.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^22.12.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.7.0",
    "jsdom": "^22.0.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.8.4",
    "monaco-editor": "0.52.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-monaco-editor": "^0.59.0",
    "rimraf": "^6.1.0",
    "rollup": "^2.78.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-import-css": "^3.3.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-visualizer": "^5.4.1",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "tslib": "^2.7.0",
    "typescript": "~5.5.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/test/**/*.test.tsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(monaco-editor)/)"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "monaco-editor": "<rootDir>/../node_modules/react-monaco-editor"
    }
  }
}
